<p id="notice"><%= notice %></p>

<h1>Identidades</h1>

<table>
  <thead>
    <tr>
      <th>Grr</th>
      <th>Cpf</th>
      <th>Discente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identidades.each do |identidade| %>
      <tr>
        <td><%= identidade.grr %></td>
        <td><%= identidade.cpf %></td>
        <td><%= identidade.discente_id %></td>
        <td><%= link_to 'Show', identidade %></td>
        <td><%= link_to 'Edit', edit_identidade_path(identidade) %></td>
        <td><%= link_to 'Excluir', identidade, method: :delete, data: { confirm: 'Tem certeza que quer deletar este registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identidade', new_identidade_path %>
